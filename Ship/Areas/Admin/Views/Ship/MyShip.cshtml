@{
    int IdUser = int.Parse(User.Identity.Name);

    if (Request.QueryString["Id"] != null)
    {

        try
        {
            int.Parse(Request.QueryString["Id"].ToString());
        }
        catch (Exception e)
        {
            Response.Redirect(@"/Admin/Default", false);
            return;
        }
    }
    else
    {
        Response.Redirect(@"/Admin/Default", false);
        return;
    }


    var dd = SiteSettingBLL.GetListShip(IdUser).Where(v => v.Id.ToString() == Request.QueryString["Id"].ToString()).ToList();


    if (dd.Any() == false)
    {
        Response.Redirect(@"/Admin/Default", false);
        return;
    }


    ViewBag.Title = "لنج " + (dd.First().Title);

    var dash1 = SiteSettingBLL.Dashboard1(IdUser);

    var IdQ = Request.QueryString["Id"].ToString();

    var dash2 = SiteSettingBLL.Dashbor3(Convert.ToInt32(IdQ));
}

